<Application x:Class="ContactManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:presenters="clr-namespace:ContactManager.Presenters"
             StartupUri="Shell.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/ColorsAndBrushes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/DefaultStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <presenters:PhoneConverter x:Key="PhoneConverter"></presenters:PhoneConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
